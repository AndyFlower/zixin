<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cul.culsite.dao.expand.MBenefitsPointsRuleDTOExpandMapper">

	<select id="getUserBenefits" resultMap="com.cul.culsite.dao.MBenefitsPointsRuleDTOMapper.BaseResultMap">
		select
		<include refid="com.cul.culsite.dao.MBenefitsPointsRuleDTOMapper.Base_Column_List" />
		from M_BENEFITS_POINTS_RULE a
		where a.mbpr_status = 'Y'
		and
		to_char(sysdate,'yyyyMMdd') between
		to_char(a.mbpr_start_date,'yyyyMMdd') and
		to_char(a.mbpr_end_date,'yyyyMMdd')
	</select>
</mapper>