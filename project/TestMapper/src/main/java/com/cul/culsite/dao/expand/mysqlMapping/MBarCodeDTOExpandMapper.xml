<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cul.culsite.dao.expand.MBarCodeDTOExpandMapper">
	<!-- update card no from inactive -->
	<update id="updateCardNo" parameterType="com.cul.culsite.dto.MBarCodeDTO">
		update M_BAR_CODE
		set NO = #{param2,jdbcType=VARCHAR}
		where TRANID = #{param1,jdbcType=VARCHAR}
	</update>
</mapper>