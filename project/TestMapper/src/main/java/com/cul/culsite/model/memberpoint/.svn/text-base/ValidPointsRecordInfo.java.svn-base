package com.cul.culsite.model.memberpoint;

import java.math.BigDecimal;
import java.util.List;

public class ValidPointsRecordInfo {

	private BigDecimal totalValidPoints;

	private List<PointsRecordBean> pointsRecordBeanList;

	public BigDecimal getTotalValidPoints() {
		return totalValidPoints;
	}

	public void setTotalValidPoints(BigDecimal totalValidPoints) {
		this.totalValidPoints = totalValidPoints;
	}

	public List<PointsRecordBean> getPointsRecordBeanList() {
		return pointsRecordBeanList;
	}

	public void setPointsRecordBeanList(
			List<PointsRecordBean> pointsRecordBeanList) {
		this.pointsRecordBeanList = pointsRecordBeanList;
	}

}
