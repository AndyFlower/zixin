package com.cul.culsite.model.memberpoint;

import java.math.BigDecimal;

public class VoucherDiscountInfo {

	private BigDecimal voucherNum;
	private String voucherName;
	private BigDecimal voucherPrice;
	private BigDecimal totalVoucherAmount;
	private String voucherType;

	public String getVoucherType() {
		return voucherType;
	}

	public void setVoucherType(String voucherType) {
		this.voucherType = voucherType;
	}

	public BigDecimal getVoucherNum() {
		return voucherNum;
	}

	public void setVoucherNum(BigDecimal voucherNum) {
		this.voucherNum = voucherNum;
	}

	public String getVoucherName() {
		return voucherName;
	}

	public void setVoucherName(String voucherName) {
		this.voucherName = voucherName;
	}

	public BigDecimal getVoucherPrice() {
		return voucherPrice;
	}

	public void setVoucherPrice(BigDecimal voucherPrice) {
		this.voucherPrice = voucherPrice;
	}

	public BigDecimal getTotalVoucherAmount() {
		return totalVoucherAmount;
	}

	public void setTotalVoucherAmount(BigDecimal totalVoucherAmount) {
		this.totalVoucherAmount = totalVoucherAmount;
	}

}
