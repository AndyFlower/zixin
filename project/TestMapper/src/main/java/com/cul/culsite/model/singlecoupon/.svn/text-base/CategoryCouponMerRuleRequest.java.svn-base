package com.cul.culsite.model.singlecoupon;

import java.util.List;

public class CategoryCouponMerRuleRequest {
	private String merId;
	private String merGroupId;
	private String issuerId;
	private List<String> couponCategoryIdList;
	public String getMerId() {
		return merId;
	}
	public void setMerId(String merId) {
		this.merId = merId;
	}
	public String getMerGroupId() {
		return merGroupId;
	}
	public void setMerGroupId(String merGroupId) {
		this.merGroupId = merGroupId;
	}
	public String getIssuerId() {
		return issuerId;
	}
	public void setIssuerId(String issuerId) {
		this.issuerId = issuerId;
	}
	public List<String> getCouponCategoryIdList() {
		return couponCategoryIdList;
	}
	public void setCouponCategoryIdList(List<String> couponCategoryIdList) {
		this.couponCategoryIdList = couponCategoryIdList;
	}

}
