<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cul.culsite.dao.expand.EsOrderItemsDTOExpandMapper">
	
	<select id="selectProductId" parameterType="com.cul.culsite.dto.EsOrderItemsDTOCriteria"
		resultMap="com.cul.culsite.dao.EsOrderItemsDTOMapper.BaseResultMap">
		select
		<if test="distinct">
			distinct
		</if>
		<include refid="com.cul.culsite.dao.EsOrderItemsDTOMapper.Base_Column_List" />
		from ES_ORDER_ITEMS
		<if test="_parameter != null">
			<include refid="com.cul.culsite.dao.EsOrderItemsDTOMapper.Example_Where_Clause" />
		</if>
	</select>
</mapper>